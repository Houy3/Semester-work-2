<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<FlowPane vgap="10"
          hgap="10"
          alignment="CENTER"
          orientation="VERTICAL"
          prefWidth="600" prefHeight="600"
          xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="com.example.clientgameapp.ChoiceController"
          >
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <Label text="Room creation form"/>


    <Button mnemonicParsing="false" prefHeight="26.0" prefWidth="152.0" text="Connect to a room" onAction="#connectToRoom"/>
    <Button mnemonicParsing="false" prefHeight="26.0" prefWidth="151.0" text="Create a room" onAction="#createRoom"/>
    <Button mnemonicParsing="false" prefHeight="26.0" prefWidth="151.0" text="Open profile" onAction="#openProfile"/>


</FlowPane>
